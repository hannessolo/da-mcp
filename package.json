{
  "name": "da-admin-mcp",
  "version": "1.0.0",
  "description": "MCP server for interacting with the Document Authoring Admin API",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "bundle": "esbuild index.ts --bundle --platform=node --target=es2018 --format=cjs --define:globalThis=global --outfile=dist/da-admin-mcp.cjs",
    "start": "node dist/index.js",
    "start:bundle": "node dist/da-admin-mcp.cjs",
    "dev": "ts-node index.ts"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.2.0",
    "node-fetch": "^3.3.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.11.5",
    "esbuild": "^0.20.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
